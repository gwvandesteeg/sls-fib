{
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "/FibonacciRequest",
  "title": "Fibonacci input schema",
  "type": "object",
  "properties": {
    "number": {
      "description": "The nth fibonacci number to calculate",
      "type": ["integer", "string"],
      "minimum": -78,
      "maximum": 78,
      "minLength": 1,
      "maxLength": 3
    }
  },
  "required": ["number"]
}